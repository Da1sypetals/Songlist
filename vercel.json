{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python3.11"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "^/$",
            "dest": "/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}